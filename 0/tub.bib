@InProceedings{GretherEtAl2009IatbrIncome,
  title = {Policy evaluation in multi-agent transport simulations considering income-dependent user preferences},
  author = {Grether, D. and Kickh{\"o}fer, B. and Nagel, K.},
  booktitle = {Proceedings of The 12th Conference of the International Association for Travel Behaviour Research ({IATBR})},
  year = {2009},

  address = {Jaipur, India},
  note = {Also VSP WP 09-13, see \url{http://www.vsp.tu-berlin.de/publications}},

  abstract = {Standard economic policy evaluation allows the realisation of projects if the aggregated economic benefit overweights their costs. In democratically organized societies, the implementation of measures with regressive effects on the welfare distribution tends to be complicated due to low public acceptance. The microscopic multi-agent simulation approach presented in this paper is capable to help designing better solutions in such situations. In particular, it is shown that income can and needs to be included in utility calculations for a better understanding of problems linked to acceptability. This paper shows how multi-agent approaches can be used in policy evaluation when including income in the user preferences. Therefore, an income-dependent utility function is estimated based on survey data. Subsequently, using the {MATSim} framework, the implementation is tested in a test scenario. Furthermore, and going beyond Franklin (2006), it is shown that the approach works in a large-scale real world example. Based on a hypothetical price and speed increase of public transit, effects on the welfare distribution of the population are discussed. It is shown that this approach, in contrast to applied economic policy analysis, allows choice modeling and economic evaluation to be realised in a consistent way.},
  crossref = {iatbr09-conference},
  url = {www.iatbr.org}
}

