@ARTICLE{Gershenson2005SelfOrgSignals,
  author = {Carlos Gershenson},
  title = {Self-Organizing Traffic Lights},
  journal = {Complex Systems},
  year = {2005},
  volume = {16},
  pages = {29--53},
  number = {1},
  abstract = {Steering traffic in cities is a very complex task, since improving
