}

@InProceedings{SteinmeyerWagner2005KiD,
  title = {{Verwendung der 'Kraftfahrzeugverkehr in Deutschland' (KiD 2002) f{\"u}r st{\"a}dtische bzw. regionale Fragestellungen}},
  author = {Steinmeyer, I. and Wagner, T.;},
  booktitle = {{Wirtschaftsverkehr 2005 -- Trends -- Modelle -- Konzepte}},
  year = {2005},
  editor = {Clausen, U},

  abstract = {In der Problemwahrnehmung der st{\"a}dtischen und regionalen Planung fristet vor allem der 'eigene' Wirtschaftsverkehr, 1 d.h. der Wirtschaftsverkehr, der sich aus den wirtschaftlichen Aktivit{\"a}ten der Betriebe und Unternehmen der jeweiligen Region ergibt, bislang eher ein Randdasein. Die Ans{\"a}tze zur Beeinflussung des Wirtschaftsverkehrs beschr{\"a}nken sich meist auf Lkw-Fernverkehre und damit auf Themen wie G{\"u}terverkehrszentren, Citylogistik, Lkw-F{\"u}hrungsnetze, Lieferzonen u.{\"a}.2 Diese Ausrichtung verschleiert die realen Probleme, die vor allem durch die gemeinsame Nutzung der Infrastruktur in Verdichtungsr{\"a}umen durch den privaten Personenverkehr und den Wirtschaftsverkehr auch und gerade mit Fahrzeugen unter 3,5 t Nutzlast entstehen.3 Hieraus ergeben sich neue Fragen und weitergehende Anforderungen an die Planung bzw. deren Instrumente. Die wenigsten St{\"a}dte halten allerdings Daten vor oder nutzen Modelle zur Abbildung des Wirtschaftsverkehrs. 4 Dennoch stellt sich die Frage nach dem Wirtschaftsverkehrsaufkommen und nach der Effektivit{\"a}t eingesetzter Ma{\"s}nahmen. Was also tun?},
  annote = {event-title=Wirtschaftsverkehr 2005 - Trends - Modelle - Konzepte; event-location=Dortmund; monograph\_type=working\_paper;},
  file = {SteinmeyerWagner2005KiD.pdf:2005/SteinmeyerWagner2005KiD.pdf:PDF}
}

@Conference{SteinmeyerWagner2005Behav,
  title = {Using National Behavioral Data on Commercial Traffic for Local and Regional Applications},
  author = {Steinmeyer, I. and Wagner, T.;},
  year = {2005},
  note = {event-title=85th TRB Annual Meeting;},
  pages = {15},

  abstract = {Commercial traffic as combination of freight and business passenger traffic represents, besides the private passenger transport, a substantial share of the complete regional traffic [% of the traffic volume]. Nevertheless, commercial traffic, which arises from the economic activities of local businesses, is often neglected by the municipalities. As a consequence, problems due to the concurrent use of the infrastructure in agglomeration areas by private passenger traffic and commercial traffic are concealed. Only very few cities and regions in Germany collect and maintain data or use traffic demand models to estimate commercial traffic. So, the question about the traffic volume and the effectiveness of measures cannot be answered. The combination of regional data about businesses and their commercial vehicles with national behavioral data regarding trips and distances per vehicle and day allows the estimation of commercial traffic caused by local businesses and thus represents a straight-forward approach to fill this gap. The data sources used are: - the Business Register with information about the meaning and the location of the businesses of the Statistical Office of Berlin, - the Central Vehicle Register of the Federal Bureau of Motor Vehicles and - national behavioral data from the 'Continuous Commercial Traffic Survey in different Settlement Areas' called 'Motor Vehicle Traffic in Germany' ('Kraftfahrzeugverkehr in Deutschland' KiD 2002). In this paper the existing data, the estimation method and the findings that can be generated will be explained and illustrated. Furthermore the possibilities and limits of the approach are critically discussed.},
  file = {SteinmeyerWagner2005Behav.pdf:2005/SteinmeyerWagner2005Behav.pdf:PDF}
}

