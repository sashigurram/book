@Conference{Steinmeyer2005Staedtischerbzwregionaler,
  title = {St{\"a}dtischer bzw. regionaler Personenwirtschaftsverkehr - Ver{\"a}nderte Mobilit{\"a}tsmuster in der Dienstleistungsgesellschaft und deren Ber{\"u}cksichtigung in der Forschungslandschaft},
