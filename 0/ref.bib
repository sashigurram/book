@TechReport{HanYuan2005Evac,
  title = {Evacuation modeling and operations using dynamic traffic assignment and most desirable destination approaches},
  author = {Han, L.D. and F. Yuan},
  year = {2005},
  number = {05-2401},

  abstract = {This paper reports the results of a microscopic evacuation simulation study in the event of a major nuclear power plant accident. The two main thrusts of this study are the use of dynamic traffic assignment (DTA) to route evacuees in a non-equilibrium environment, which is the case for unplanned or 'no-notice' large-scale disasters, and the consideration of most desirable destinations (MDD) for all evacuees in a transient traffic network, where a nearby shelter may not be the most desirable depending on the network condition at the time. By affording flexibilities in the route assignment and destination selection processes, this study demonstrates feasibility and superiority of using DTA and MDD with microscopic simulation of traffic evacuation operations. For the study site of Sequoyah nuclear power plant in Tennessee, a 58% reduction in overall evacuation time was achieved when DTA and MDD were implemented. This paper also details the steps involved in preparing necessary data for the proper representation of the transportation network, the evacuation traffic demand, and other tangible aspects for various evacuation scenarios. TRB 2005 Annual Meeting CD-ROM Paper revised from original submittal.},
  crossref = {trb},
  file = {HanYuan2005Evac.pdf:2005/HanYuan2005Evac.pdf:PDF},
