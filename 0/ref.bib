@Conference{LuKawamura2007UrbanFormTravelBehavior,
  title = {The influence of urban forms on work travel behavior: an exploration using agent-based modeling},
  author = {Lu, Yandan and Kawamura, Kazuya;},
  year = {2007},
  note = {I started writing the following. That argument is, however, NOT correct because people will, based on car ownership, self-select into the appropriate locations. Kai There is one slightly larger issue that needs, in my view, more discussion in the paper: In Sec. 2.3, car ownership is assigned randomly to households. This means, for example, that we can easily have someone without a car AND without transit access, or someone with transit access who might give up a car in reality but will keep it in the model. At least the following will be the result: - average transit travel times will be overestimated since too many people will be forced (because of no car) to use transit from inconvenient locations - transit usage near transit stops will be under-estimated, because "too many" people will have a car The workplace location plays a similar role: People who have a transit-accessible workplace might self-select into transit-accessible housing, and then give up the car. See Cao et al for the residential location/car ownership self selection, and Wet et al for work location relevance. I am not saying that the model needs to be modified at this stage, only that the above issues should be discussed prominently in the paper. ===== ; event-title=Submitted for TRB'08;},

  file = {LuWorkTravelBehav:2007/LuWorkTravelBehav.pdf:PDF}
}

