@mastersthesis{Schlund2005PrognoseverfahrenzurAbschaetzung,
  author =        {Schlund, S.},
  note =          {thesisType=other; department=FG Verkehrssystemplanung
                   und Verkehrstelematik;},
  pages =         {74},
  school =        {TU Berlin},
  type =          {Term Project Report},
  title =         {Prognoseverfahren zur Absch{\"a}tzung der
                   verkehrlichen Auswirkungen von City-Maut-Systemen},
  year =          {2005},
  abstract =      {Weltweit leiden St{\"a}dte und Ballungsgebiete unter
                   der sich immer weiter verst{\"a}rkenden
                   Verkehrsbelastung. Den betroffenen Volkswirtschaften,
                   deren Zentren nahezu lahm gelegt werden, entstehen
                   somit Sch{\"a}den in Millionenh{\"o}he.
                   Zus{\"a}tzlich steigt die Umweltbelastung durch
                   Ger{\"a}usch- und Schadstoffemissionen. Im
                   Mittelpunkt eines jeden Ansatzes zur Erhaltung der
                   Mobilit{\"a}t und Verbesserung der Verkehrssituation
                   in den Innenst{\"a}dten steht der {\"O}ffentliche
                   Personennahverkehr ({\"O}PNV). Doch nachdem schon
                   jahrzehntelang {\"o}ffentliche Mittel verst{\"a}rkt
                   in dessen Verbesserung geflossen sind, sehen sich die
                   Kommunen mittlerweile verst{\"a}rkt mit einer
                   angespannten Finanzsituation konfrontiert. Des
                   Weiteren hatte der Ausbau der {\"o}ffentlichen
                   Verkehrsmittel nicht den gew{\"u}nschten Erfolg - mit
                   Sicherheit auch, weil der PKW als Fortbewegungsmittel
                   des Motorisierten Individualverkehrs (MIV) weiterhin
                   als {\"u}berlegen wahrgenommen wird. Eine nachhaltige
                   Verschiebung des Modal Splits zugunsten des {\"O}PNV
                   kann somit nur {\"u}ber eine Verbesserung des
                   {\"O}PNV bei gleichzeitiger Restriktion des MIV
                   stattfinden. In diesem Spannungsfeld gewinnen mit
                   zunehmender Belastung der betroffenen
                   Gebietsk{\"o}rperschaften City-Maut-Systeme, als auf
                   die Innenstadtbereiche beschr{\"a}nkte Auspr{\"a}gung
                   von Stra{\"s}enbenutzungsgeb{\"u}hren, immer mehr
                   Bedeutung. Obwohl bereits seit den 70er Jahren
                   City-Maut-Systeme existieren, erlangte die
                   Einf{\"u}hrung des London Congestion Charging Scheme
                   (CCS) am 17.Februar 2003 erh{\"o}hte Aufmerksamkeit.
                   Erstmals wurde hiermit in einer europ{\"a}ischen
                   Metropole ein auf einer Gebietslizenz basierendes
                   System eingef{\"u}hrt. Da sich nach nunmehr
                   dreij{\"a}hrigem Betrieb die Verkehrsbelastung in der
