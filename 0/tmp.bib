@article{ZhangEtAl2012BiPedFlows,
  author =        {J Zhang and W Klingsch and A Schadschneider and
                   A Seyfried},
  journal =       {Journal of Statistical Mechanics: Theory and
                   Experiment},
  number =        {02},
  pages =         {P02002},
  title =         {Ordering in bidirectional pedestrian flows and its
                   influence on the fundamental diagram},
  volume =        {2012},
  year =          {2012},
  abstract =      {Experiments under laboratory conditions were carried
                   out to study the ordering in bidirectional pedestrian
                   streams and its influence on the fundamental diagram
                   (density-speed-flow relation). The Voronoi method is
                   used to resolve the fine structure of the resulting
                   velocity-density relations and spatial dependence of
                   the measurements. The data show that the specific
                   flow concept is applicable also for bidirectional
                   streams. For various forms of ordering in
                   bidirectional streams, no large differences among
                   density-flow relationships are found in the observed
                   density range. The fundamental diagrams of
                   bidirectional streams with different forms of
                   ordering are compared with those of unidirectional
                   streams. The result shows differences in the shape of
                   the relation for rho > 1.0m - 2 . The maximum of the
                   specific flow in unidirectional streams is
                   significantly larger than that in all bidirectional
                   streams examined.},
  url =           {http://stacks.iop.org/1742-5468/2012/i=02/a=P02002},
}

