@mastersthesis{Pavlov2011Ma,
  address =       {Berlin, Germany},
  author =        {Pavlov, V.},
  crossref =      {DiplomArbeitVsp},
  month =         {Juni},
  school =        {TU Berlin, Institute for Land and Sea Transport
                   Systems},
  type =          {Diplomarbeit ({D}iploma {T}hesis)},
  title =         {Verkehrsinformationssysteme im {\"o}ffentlichen
                   {P}ersonennahverkehr {{\"O}PNV}},
  year =          {2011},
  abstract =      {Trotz des steigenden Umweltbewusstseins der
                   Bev{\"o}lkerung in den letzten Jahren nutzen immer
                   noch vergleichsweise wenige Menschen in Deutschland
                   den {\"O}PNV. Mit einem Anteil von 9% am
                   Verkehrsaufkommen im Personenverkehr liegt der
                   {\"O}PNV deutlich hinter dem MIV mit seinen 58%1.
                   Auch wenn in urbanen R{\"a}umen diese Werte dank
                   gutem {\"O}PNV-Angebot wesentlich h{\"o}her liegen,
                   bleiben sie unter dem Pkw-Niveau [vgl. infas/DLR
                   2010]. Einer der Gr{\"u}nde f{\"u}r die
                   {\"O}PNV-Nichtnutzung liegt darin, dass die
                   Beschaffung erforderlicher Informationen und Umgang
                   mit ihnen teilweise als zu kompliziert empfunden
                   werden. Eine schlechte Informiertheit {\"u}ber das
                   vorhandene {\"O}PNV-Angebot f{\"u}hrt ebenfalls dazu,
                   dass dieses Angebot nicht wahrgenommen wird. Auch
                   routinierte {\"O}PNV-Nutzer sind oft mit vorhandenen
                   Informationen und Informationsmedien unzufrieden Vor
                   diesem Hintergrund sollte am Beispiel des Berliner
                   {\"O}PNV untersucht werden, welche
                   Informationssysteme derzeit im {\"O}PNV eingesetzt
                   werden und welche Hemmfaktoren zur Nutzung des
                   {\"O}PNV, die beim Umgang mit diesen Systemen
                   entstehen, die gr{\"o}\ss{}ten sind. Die
                   {\"O}PNV-Nutzer und {\"O}PNVNichtnutzer sollten dabei
                   differenziert betrachtet werden. Ein weiteres Problem
                   bei der Nutzung des {\"O}PNV besteht darin, dass es
                   nur verh{\"a}ltnism{\"a}\ss{}ig wenige Ziele mit
                   einer Direktverbindung erreicht werden k{\"o}nnen.
                   Eins der gr{\"o}\ss{}ten Hindernisse f{\"u}r viele
                   {\"O}PNV-Nutzer stellen der Umsteigezwang und die
                   damit verbundenen Wartezeiten dar. Dynamische
                   Anschlussanzeiger an den Umsteigestationen der
                   Berliner U-Bahn sind speziell daf{\"u}r entwickelt
                   worden, die Fahrg{\"a}ste beim Umsteigen besser
                   {\"u}ber die aktuellen Abfahrtszeiten der
                   Anschlusslinien zu informieren. Dieses
                   Informationsmedium sollte nun im n{\"a}chsten Schritt
                   genauer angeschaut werden werden. Zu kl{\"a}renden
                   Fragen waren dabei, wie die Fahrg{\"a}ste dieses
                   Medium in heutiger Form beurteilen und welche
                   Anforderungen sie an (k{\"u}nftige) Anschlussanzeiger
                   stellen.},
}

