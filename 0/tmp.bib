@techreport{RizziSteinmetz2015VOTIncomePreferenceEquity,
  author =        {Rizzi, L.I. and Steinmetz, S.S.C.},
  institution =   {Pontificia Universidad Cat{\'o}lica de Chile,
                   California State University},
  title =         {On Using Standard Values of Time in Project
                   Appraisal: Income Equity vs. Preference Equity},
  year =          {2015},
  abstract =      {We examine the practice of using ``standard''
                   or``equity'' values of time to evaluate the benefits
                   of transportation improvements, and we make explicit
                   the social weighting schemes that are consistent with
                   that practice. We demonstrate that if travel-time
                   preferences vary, then standard values of time
                   typically imply social weighting schemes that favor
                   those who gain the least utility from travel-time
                   savings. We further show how this finding persists
                   when improvements are financed by user payments such
                   as road tolls and taxes. We then describe a remedial
                   weighting scheme that generalizes several evaluation
                   approaches developed in the literature.},
}

