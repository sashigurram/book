@techreport{ZumkellerMoP2006,
  author =        {Dirk Zumkeller},
  institution =   {Bundesministers f{\"u}r Verkehr, Bau- und
                   Stadtentwicklung},
  month =         {11},
  note =          {MoP},
  title =         {{Panelauswertung 2006 (Mobilit{\"a}tspanel)}},
  year =          {2007},
  abstract =      {Ausgangslage F{\"u}r verkehrspolitische
                   Weichenstellungen ben{\"o}tigen
                   Entscheidungstr{\"a}ger aktuelle Informationen
                   {\"u}ber das Mobilit{\"a}tsverhalten der
                   Bev{\"o}lkerung. Wichtig f{\"u}r eine menschen- und
                   umweltgerechte Verkehrsplanung sind dabei nicht nur
                   Kenntnisse {\"u}ber die kollektive sondern auch
                   {\"u}ber die individuelle Verhaltensvielfalt bei
                   Aktivit{\"a}tsaus{\"u}bung und Mobilit{\"a}t.
                   Dar{\"u}ber hinaus sind f{\"u}r Verkehrsplanungen
                   vielfach Einblicke in Ursachen von Entwicklungen und
                   Kenntnisse der Wirkungen von exogenen Einfl{\"u}ssen
                   wie der Wirtschaftsentwicklung oder demographischen
                   Ver{\"a}nderungen notwendig. Zur Panelmethode Vor
                   diesem Hintergrund wurde f{\"u}r das Deutsche
                   Mobilit{\"a}tspanel (MOP) eine Kombination von
                   L{\"a}ngsschnitt- und Panelerhebung gew{\"a}hlt. Seit
                   1994 werden private Haushalte in drei aufeinander
                   folgenden Jahren {\"u}ber den Zeitraum einer Woche zu
                   ihrem Mobilit{\"a}tsverhalten befragt. Auf dieser
                   Grundlage liefert das MOP einerseits aktuelle
                   Information {\"u}ber den Ist-Zustand kollektiven als
                   auch individuellen Mobilit{\"a}tsverhaltens. Es
                   erlaubt dar{\"u}ber hinaus auch Einblicke in
                   Ursache-Wirkungs-Zusammenh{\"a}nge und
                   l{\"a}ngerfristige Entwicklungen im
                   Mobilit{\"a}tsverhalten. Zu Stichprobe und Daten Mit
                   der Methode des MOP werden aktuelle und im Zeitablauf
                   vergleichbare Informationen zum Personenverkehr
                   privater Haushalte gesammelt. In den letzten Jahren
                   wurden jeweils zwischen 750 und {\"u}ber 1.000
                   Haushalten zu ihrer Alltagsmobilit{\"a}t befragt. Es
                   nahmen j{\"a}hrlich ca. 1500 Personen teil, die etwa
                   37.000 Wege zur{\"u}cklegten. Ein Teil der Haushalte
                   in der Stichprobe wird dar{\"u}ber hinaus auch zur
                   Nutzung und zum Kraftstoffverbrauch ihrer PKW
                   befragt. Im Rahmen dieser "Tankbucherhebung" wurden
                   j{\"a}hrlich bei ca. 350 PKW Daten zu Fahrleistung
                   und Kraftstoffverbr{\"a}uchen gesammelt.},
  url =           {http://mobilitaetspanel.ifv.uni-karlsruhe.de/wissenschaft/
                  indexwissenschaft.htm},
}

