@article{Sugiman1988FollowMeEvacuation,
  author =        {Sugiman, Toshio and Misumi, Jyuji},
  journal =       {Journal of Applied Psychology},
  number =        {1},
  pages =         {3--10},
  title =         {Development of a New Evacuation Method for
                   Emergencies: Control of Collective Behavior by
                   Emergent Small Groups.},
  volume =        {73},
  year =          {1988},
  abstract =      {A new evacuation method for emergencies, the Follow
                   Me method, in which each leader takes only one
                   evacuee to an exit without gesturing or loudly
                   directing many evacuees toward the exit, was
                   compared, in two field experiments, with a
                   traditional method, the Follow Directions method, in
                   which each leader indicates the direction with a loud
                   voice and vigorous gestures. Results showed that the
                   Follow Me method was more effective when the
                   leader-to-evacuee ratio was relatively small, such as
                   1:4, and less effective when the ratio was large,
                   such as 1:8. Effective evacuation by the Follow Me
                   method was shown to be caused by the drawing power of
                   emergent small groups. Generating from around a
                   leader, other evacuees rapidly formed a collective
                   stream toward the exit. Small groups can be used as a
                   lever in moving a larger collectivity in an
                   emergency, as well as m activating a large formal
                   organization. ABSTRACT FROM AUTHOR},
  issn =          {00219010},
  url =           {http://search.ebscohost.com/
                  login.aspx?direct=true\&db=buh\&AN=5173132\&site=ehost-live},
}

