@article{LuGeorgeEtAl2005CapacityConstrainedRouting,
  author =        {Lu, Q. and George, B. and Shekhar, S.},
  journal =       {LNCS},
  pages =         {291-307},
  title =         {Capacity Constrained Routing Algorithms for
                   Evacuation Planning: A Summary of Results},
  volume =        {3633},
  year =          {2005},
  abstract =      {Abstract. Evacuation planning is critical for
                   numerous important ap- plications, e.g. disaster
                   emergency management and homeland defense
                   preparation. E=EF=AC=83cient tools are needed to
                   produce evacuation plans that identify routes and
                   schedules to evacuate a=EF=AC=80ected populations to
                   safety in the event of natural disasters or terrorist
                   attacks. The existing linear programming approach
                   uses time-expanded networks to compute the op- timal
                   evacuation plan and requires a user-provided upper
                   bound on evac- uation time. It su=EF=AC=80ers from
                   high computational cost and may not scale up to large
                   transportation networks in urban scenarios. In this
                   paper we present a heuristic algorithm, namely
                   Capacity Constrained Route Planner(CCRP), which
                   produces sub-optimal solution for the evacua- tion
                   planning problem. CCRP models capacity as a time
                   series and uses a capacity constrained routing
                   approach to incorporate route capacity constraints.
                   It addresses the limitations of linear programming
                   approach by using only the original evacuation
                   network and it does not require prior knowledge of
                   evacuation time. Performance evaluation on various
                   network con=EF=AC=81gurations shows that the CCRP
                   algorithm produces high quality solutions, and
                   signi=EF=AC=81cantly reduces the computational cost
                   com- pared to linear programming approach that
                   produces optimal solutions. CCRP is also scalable to
                   the number of evacuees and the size of the network.},
}

